<div class="pushright">
	<table class="table table-striped">
			<tbody>
				<% Contribution.all.where(:user =>current_user).order("created_at DESC").each do |contribution| %>
				<tr>
					<td><%=contribution.created_at.strftime("%H:%M")%></td>
					<td><%=contribution.created_at.strftime("%d %b. %Y")%></td>
					<td>
						<%= link_to contribution.event.name, contribution.event%>
					</td>
					<td style="text-align:right;">R$ 45,90</td>
				</tr>
				<%end%>
			</tbody>
		</table>
</div>